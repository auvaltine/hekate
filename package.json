{
	"name": "hekate",
	"version": "1.0.0",
	"description": "JavaScript witchcraft: magically lightweight, nimble, comprehensive, full stack web framework.",
	"homepage": "https://hekate.app/@core",
	"funding": "https://hekate.app/donate",
	"repository": {
		"type": "git",
		"url": "https://hekate.app/r/hekate"
	},
	"bugs": {
		"email": "hi@hekate.app"
	},
	"license": "ISC",
	"author": {
		"name": "Abhayā Auva",
		"email": "abhaya@hekate.app",
		"url": "https://hekate.app"
	},
	"config": {
		"allowed": [ "/(robots|sitemap).txt", "/content/modules/client/.*" ],
		"cluster": 0,
		"environment": "development",
		"http": { "port": 80 },
		"https": { "port": 443 },
		"log": {
			"cycle": 7,
			"http": "%a \"%r\" %s %B %Fms \"%f\" \"%u\" %d",
			"size": 10e+6,
			"trim": 256,
			"trimText": "%s more characters"
		},
		"restricted": [ ".*/[^/]+\\.[a-z]+$" ],
		"session": { "expires": 15 },
		"template": { "directory": "/assets", "client": true, "js": [], "css": [] },
		"upload": { "max": 4e+6, "file": 2e+6 },
		"view": {}
	},
	"os": [ "!win32" ],
	"engines": {
		"node": ">=10.10.0"
	},
	"scripts": {
		"postinstall": "./index.mjs postinstall",
		"update": "./index.mjs update",
		"restart": "./index.mjs restart",
		"start": "./index.mjs start",
		"stop": "./index.mjs stop",
		"build": "./index.mjs build"
	},
	"dependencies": {
		"@babel/core": "^7.19.0",
		"babel-preset-minify": "^0.5.0",
		"mongodb": "^4.9.0",
		"sass": "^1.54.0",
		"semver": "^7.3.8"
	}
}
